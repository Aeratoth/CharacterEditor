<Window x:Class="WpfGui.Views.SelectCharacterDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelectCharacterDialog" Height="300" Width="300"
        Topmost="True"
        WindowStartupLocation="CenterScreen"
        DataContext="{Binding DesignSelectCharacterDialog, Source={StaticResource Locator}}">
    <Grid>
        <ListBox ItemsSource="{Binding Characters}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBox Text="{Binding Name}"></TextBox>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>